{
	"name": "n8n-nodes-binance",
	"version": "0.1",
	"description": "n8n node to trade on Binance",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/glazec/n8n-nodes-binance",
	"author": {
		"name": "Test",
		"email": "test@example.com"
	},
	"repository": {
		"type": "git",
		"url": "git+<your-repo-url>"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"test": "npm run build;npm link;cd ~/.n8n/nodes/;npm link n8n-nodes-binance;export N8N_LOG_LEVEL=info;n8n start"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/BinanceApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Binance/Binance.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/request-promise-native": "~1.0.15",
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-core": "*",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"dependencies": {
		"@binance/connector": "^3.0.0",
		"@types/request": "^2.48.8",
		"node-fetch": "^3.3.1",
		"request": "^2.88.2"
	}
}